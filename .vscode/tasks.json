    {
      "type": "swift",
      "args": ["test"],
      "env": {},
      "cwd": "${workspaceFolder}",
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "label": "swift: test",
      "detail": "swift test"
    }
  ]
}
