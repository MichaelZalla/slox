{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "swift",
      "args": ["run", "slox", "scripts/hello.lox"],
      "env": {},
      "cwd": "${workspaceFolder}",
      "disableTaskQueue": true,
      "dontTriggerTestDiscovery": true,
      "showBuildStatus": "swiftStatus",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [],
